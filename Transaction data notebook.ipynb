    "#define function to count number and percentage of Na values\n",
    "def summarize_na(df: pd.DataFrame) -> pd.DataFrame:\n",
    "    nan_count = df.isna().sum()\n",
    "    return pd.DataFrame({'nan_count': nan_count, \n",
    "                         'nan_pct': nan_count / len(df) * 100\n",
    "                         }\n",
    "                        )[nan_count > 0]"
   ]
  },
